{{~#if parameters}}
	{{~#if parametersPath}}
		{{~#each parametersPath}}
  			{{~{name}}}{{#unless @last}},{{/unless}}
		{{~/each}}
	{{~/if}}
	{{~#if parametersBody}}
		{{~#if parametersPath}},{{/if}}
		{{~#with parametersBody}}
			{{~{name}}}
		{{~/with}}
		{{~#if parametersQuery}},{{/if}}
	{{~/if}}
	{{~#if parametersQuery}}
		{{~#if parametersPath}}{{#unless parametersBody}},{{/unless}}{{/if~}}
		queryParams
	{{~/if}}
{{~/if}}
